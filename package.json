{
  "name": "nyc_outward",
  "version": "1.0.0",
  "description": "###Description###",
  "main": "prodServer.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc mocha test/server/app_test.js && karma start",
    "test:watch": "npm run test -- --watch",
    "watch": "nodemon --exec babel-node app/server/server.js",
    "test-server": "cross-env NODE_ENV=test nyc mocha test/server/app_test.js",
    "test-client": "karma start",
    "postinstall": "per-env",
    "postinstall:development": "echo 'skipping postinstall as in development'",
    "postinstall:test": "echo 'skipping postinstall as in test'",
    "postinstall:production": "npm run build",
    "debug": "node-inspector --web-port=8888 & nodemon --config nodemon.json --exec babel-node --debug-brk server.js",
    "build-server": "babel app --out-dir appDist --ignore app/client --source-maps",
    "build-client": "webpack -p --config ./webpack.prod.config.js",
    "build": "npm run build-server && npm run build-client",
    "start": "NODE_ENV=production node appDist/server/server.js --max-old-space-size=1000",
    "babel": "./node_modules/.bin/babel-node",
    "format": "prettier --single-quote --tab-width 4 --print-width 140 --write './*.js'",
    "watch:test": "webpack-dev-server --progress",
    "import": "babel-node ./utils/import_data.js"
  },
